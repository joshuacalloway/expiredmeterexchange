<h1>Listing payment_notifications</h1>

<table>
  <tr>
    <th>Params</th>
    <th>transaction_id</th>
    <th>Status</th>
    <th>txn_id</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @payment_notifications.each do |payment_notification| %>
  <tr>
    <td><%= payment_notification.params %></td>
    <td><%= payment_notification.transaction_id %></td>
    <td><%= payment_notification.status %></td>
    <td><%= payment_notification.txn_id %></td>
    <td><%= link_to 'Show', admin_payment_notification_path(payment_notification) %></td>
    <td><%= link_to 'Edit', edit_admin_payment_notification_path(payment_notification) %></td>
    <td><%= link_to 'Destroy', admin_payment_notification_path(payment_notification), confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

